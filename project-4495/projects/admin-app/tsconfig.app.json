{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "../../out-tsc/app",
    "types": [],
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@asset/*": [
        "src/asset/*"
      ],
      "@environment/*": [
        "src/environments/*"
      ],
      "library-app": [
        "../../dist/library-app/library-app",
        "../../dist/library-app"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
